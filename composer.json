{
    "name": "devtransition/yii2-rabbitmq",
    "description": "Yii 2 extension wrapper for the awesome RabbitMQ server via AMQP. Based on videlalvaro/php-amqplib.",
    "keywords": [
        "yii",
        "yii2",
        "extension",
        "amqp",
        "RabbitMQ"
    ],
    "homepage": "https://github.com/devTransition/yii2-rabbitmq",
    "license": "Apache-2.0",
    "support": {
        "issues": "https://github.com/devTransition/yii2-rabbitmq/issues"
    },
    "authors": [
        {
            "name": "Nicolas Wild",
            "email": "nwild79@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "php-amqplib/php-amqplib": "2.6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "1.3.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "devtransition\\rabbitmq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "devtransition\\rabbitmq\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
